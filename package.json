{
  "name": "turbodoc-browser-extension",
  "version": "1.0.1",
  "description": "Turbodoc Browser Extension - One-click bookmarking for the modern web",
  "private": true,
  "scripts": {
    "build": "node build/build-extension.js build",
    "build:chrome": "node build/build-extension.js build chrome",
    "build:firefox": "node build/build-extension.js build firefox",
    "package": "node build/build-extension.js package",
    "dev": "node build/build-extension.js dev",
    "clean": "node build/build-extension.js clean",
    "info": "node build/build-extension.js info",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "bookmark",
    "browser-extension",
    "chrome",
    "firefox",
    "productivity"
  ],
  "author": "Turbodoc",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
